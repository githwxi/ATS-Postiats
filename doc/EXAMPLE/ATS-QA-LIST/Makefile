cleanall:: ; $(RMF) calc_refcnt

######
#
CFLAGSjson += $(shell pkg-config --cflags json-c)
#
all:: for-0install
for-0install: \
  for-0install.dats ; \
  $(PATSCC) -D_BSD_SOURCE -DATS_MEMALLOC_LIBC -IIATS ${PATSHOME}/contrib $(CFLAGSjson) -o $@ $< -ljson-c
cleanall:: ; $(RMF) for-0install
#
######

RMF=rm -f

######

clean:: ; $(RMF) *~
clean:: ; $(RMF) *_?ats.c
clean:: ; $(RMF) *_?ats.o

cleanall:: clean

###### end of [Makefile] ######
